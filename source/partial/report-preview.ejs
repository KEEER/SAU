<a href="/report/<%= report.id %>" class="mdc-list-item report<%= (report.score !== undefined || user.role === "association")?"":" unread mdc-theme--primary" %>" data-mdc-auto-init="MDCRipple">
  <span class="mdc-list-item__text">
      <span class="mdc-list-item__primary-text"><%= report.title %></span>
      <span class="mdc-list-item__secondary-text"><%= new Date(report.stime).toLocaleString("zh") %></span>
  </span>
  <i class="open material-icons mdc-icon-button" title="Open">open_in_new</i>
</a>
<hr class="mdc-list-divider">

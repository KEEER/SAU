<div class="mdc-snackbar mdc-snackbar--align-start"
     aria-live="assertive"
     aria-atomic="true"
     aria-hidden="true"
     data-mdc-auto-init="MDCSnackbar">
  <div class="mdc-snackbar__text"></div>
  <div class="mdc-snackbar__action-wrapper">
    <button type="button" class="mdc-snackbar__action-button"></button>
  </div>
</div>
<script src="https://unpkg.com/material-components-web@latest/dist/material-components-web.min.js"></script>
<script>mdc.autoInit();</script>
<script>
window.snackbar = document.querySelector(".mdc-snackbar").MDCSnackbar;
window.drawerEl = document.getElementById('drawer');
window.drawer = window.drawerEl.MDCDrawer;
window.onresize = function() {
  var width = window.innerWidth;
  if(width > 720) {
    drawer.open=!0;
  } else {
    drawer.open=!1;
  }
};
drawerEl.style.transitionDuration = "1ms";
window.onresize();
setTimeout(function(){
  drawerEl.style.transitionDuration = "";
  document.body.classList.add("ready");
},100);
</script>
